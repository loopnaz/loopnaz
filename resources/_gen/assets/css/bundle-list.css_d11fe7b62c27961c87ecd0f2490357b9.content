html{box-sizing:border-box;line-height:1.15;-webkit-text-size-adjust:100%}*,*:before,*:after{box-sizing:inherit}body,h1,nav ul{margin:0}figure,h2,h3,h4,h5,iframe{margin:0 0 1em;max-width:100%}section h2{width:100%}li h3{margin:0;display:inline}a{background-color:transparent}img{border-style:none}section p{font-size:larger;margin-top:0}html{height:100%}body{min-height:100%;display:grid;grid-template-rows:1fr auto}body>footer{grid-row-start:-1}main>article,.content,main>section,.menu ul:not(.hidden),p img{display:flex}main>article,.content,:not(.home)>main>section,.menu ul{flex-direction:column}main>article,.content,:not(.home)>main>section{align-items:center}header,img,main,p{width:100%}.hidden{display:none;visibility:hidden}html{font-family:raleway,sans-serif}h1{font-size:3em}h1,.menu ul li{text-align:center}button,blockquote::before,blockquote::after,blockquote span,.menu ul li:last-of-type a{font-weight:600}button{font-family:inherit;font-size:100%;line-height:1.15;margin:0}blockquote{color:grey;position:relative;max-width:calc(100% - 4em);margin:1em 0}blockquote::before,blockquote::after{font-size:3em;position:absolute}blockquote::before{content:"\201C";top:-.15em;left:-.5em}blockquote::after{content:"\201D";bottom:-.25em;right:-.5em}cite{font-style:normal}blockquote figure{float:right}blockquote cite{display:block;font-size:smaller;text-align:right}blockquote cite:before{content:"\2014"}.x-large{font-size:x-large}main ol,main ul{margin:0 auto 1em 0}main ul{list-style:square url(/img/svg/chicago-star.svg) outside}main>article{counter-reset:figure}figure:not(.avatar) figcaption::before{content:"Figure " counter(figure,decimal) ". ";counter-increment:figure}figure:not(.avatar) figcaption::before,figcaption *{font-size:smaller;font-weight:600;color:#acacac}figure:not(.avatar) figcaption *{display:inline}nav ul{list-style:none;padding-left:0}nav ul li a{text-decoration:none}nav button{position:absolute;top:0;right:0}button,.menu ul li:last-of-type a{background:transparent;border:2px solid #fff;padding:3px 7px;margin:2px;color:#fff;border-radius:13px}.menu ul:not(.hidden){background-color:#000;background-color:rgba(0,0,0,.77)}.menu ul{justify-content:space-around;position:absolute;width:100%;top:32px}.menu ul li{padding:.5em 0}header a{color:inherit}.site-logo{max-width:200px}main a{color:#00a1de;font-weight:600}main a:focus,main a:hover{color:#c60c30}main a:visited,footer a:not(.social):visited{color:#522398}header a:focus,header a:hover,footer a:focus,footer a:focus,button:focus,button:hover{cursor:pointer;opacity:.77}footer a{text-decoration:none}figure svg{max-width:100%;height:auto}.blue svg{fill:#00a1de}.red svg{fill:#c60c30}.white svg{fill:#fff}.background-image{background-repeat:no-repeat;background-position:top;background-size:cover}body>header{background-color:#000;background-color:rgba(0,0,0,.77);color:#fff;height:32px;position:fixed;top:0;z-index:999}body>header svg{fill:#fff;height:100%;width:auto;padding:2px}body>footer{background-color:#eee;font-size:smaller;padding:0 1em;display:flex;flex-direction:column;justify-content:space-between;align-content:center;min-height:160px}.content{width:calc(100% - 2em);max-width:49.5em}main h1{padding-top:32px}main>article header.background-image{color:#fff;text-shadow:1px 1px 2px rgba(0,0,0,.77);min-height:320px;height:50vh;display:flex;flex-direction:column;justify-content:center;width:100%;margin-bottom:1em}article header.background-image h1{flex:0 0 auto;margin:auto;padding-top:32px}article header.background-image p{font-weight:900;text-align:right}main+aside{grid-row-start:3}p img{margin:0 auto}.red{color:#c60c30}.blue{color:#00a1de}footer nav{width:66vw;margin:auto}footer nav ul,.social ul{display:flex;justify-content:space-evenly}.social svg{fill:#bcbcbc;height:1.5em;width:auto}.twitter svg:hover{fill:#55acee}.facebook svg:hover{fill:#3b5999}.linkedin svg:hover{fill:#0077b5}.instagram svg:hover{fill:#fbad50}.github svg:hover{fill:#171515}.google-plus svg:hover{fill:#dd4b39}main .center{grid-column-start:1;grid-column-end:-1;min-height:50vh}@media screen and (min-width:640px){body>header{display:flex;justify-content:space-between}nav button{display:none;visibility:hidden}.menu{flex:1 0 auto;margin:auto}.menu ul{display:flex;flex-direction:row;visibility:visible;background-color:unset!important;position:unset!important}.menu ul li{padding:unset}}@media screen and (min-width:800px){body>footer{flex-direction:row;min-height:unset}footer nav{width:33vw}}footer :not(.social) a:hover{color:#00a1de;text-decoration:underline}.give-button a{background:#00a1de;border:2px solid #00a1de;border-radius:3em;color:#fff!important;display:block;width:25vw;min-width:250px;max-width:calc(100% - 2em);margin:0 auto;text-align:center;text-decoration:none;padding:1em;box-shadow:3px 2px 1px rgba(0,0,0,.5)}.give-button a:hover{background:#c60c30;border:2px solid #c60c30;box-shadow:5px 4px 3px rgba(0,0,0,.5)}main nav,main+nav{margin:1em;max-width:100%}.partner main nav{width:100%}main nav ul,main+nav ul{display:grid;grid-template-columns:repeat(auto-fit,minmax(calc(320px - 2em),1fr));grid-gap:1em}main nav ul li,main+nav ul li{font-size:2em;font-weight:600;text-align:center}main nav ul li a,main+nav ul li a{min-height:calc(320px - 2em);color:#fff;text-shadow:1px 1px 2px rgba(0,0,0,.77);display:flex;flex-direction:column;justify-content:center;padding:0 1em}main nav ul li a svg{max-width:100%;height:auto}main+nav ul li:nth-of-type(7n+1) a{background:rgba(198,12,48,.77)}main+nav ul li:nth-of-type(7n+2) a{background:rgba(0,161,222,.77)}main+nav ul li:nth-of-type(7n+3) a{background:rgba(98,54,27,.77)}main+nav ul li:nth-of-type(7n+4) a{background:rgba(0,155,58,.77)}main+nav ul li:nth-of-type(7n+5) a{background:rgba(249,70,28,.77)}main+nav ul li:nth-of-type(7n+6) a{background:rgba(82,35,152,.77)}main+nav ul li:nth-of-type(7n+7) a{background:rgba(226,126,166,.77)}main+nav ul li a:hover{opacity:1;background:0 0}