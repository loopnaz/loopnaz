html{box-sizing:border-box;line-height:1.15;-webkit-text-size-adjust:100%}*,*:before,*:after{box-sizing:inherit}body,h1,nav ul{margin:0}figure,h2,h3,h4,h5,iframe{margin:0 0 1em;max-width:100%}section h2{width:100%}li h3{margin:0;display:inline}a{background-color:transparent}img{border-style:none}section p{font-size:larger;margin-top:0}html{height:100%}body{min-height:100%;display:grid;grid-template-rows:1fr auto}body>footer{grid-row-start:-1}main>article,.content,main>section,.menu ul:not(.hidden),p img{display:flex}main>article,.content,:not(.home)>main>section,.menu ul{flex-direction:column}main>article,.content,:not(.home)>main>section{align-items:center}header,img,main,p{width:100%}.hidden{display:none;visibility:hidden}html{font-family:raleway,sans-serif}h1{font-size:3em}h1,.menu ul li{text-align:center}button,blockquote::before,blockquote::after,blockquote span,.menu ul li:last-of-type a{font-weight:600}button{font-family:inherit;font-size:100%;line-height:1.15;margin:0}blockquote{color:grey;position:relative;max-width:calc(100% - 4em);margin:1em 0}blockquote::before,blockquote::after{font-size:3em;position:absolute}blockquote::before{content:"\201C";top:-.15em;left:-.5em}blockquote::after{content:"\201D";bottom:-.25em;right:-.5em}cite{font-style:normal}blockquote figure{float:right}blockquote cite{display:block;font-size:smaller;text-align:right}blockquote cite:before{content:"\2014"}.x-large{font-size:x-large}main ol,main ul{margin:0 auto 1em 0}main ul{list-style:square url(/img/svg/chicago-star.svg) outside}main>article{counter-reset:figure}figure:not(.avatar) figcaption::before{content:"Figure " counter(figure,decimal) ". ";counter-increment:figure}figure:not(.avatar) figcaption::before,figcaption *{font-size:smaller;font-weight:600;color:#acacac}figure:not(.avatar) figcaption *{display:inline}nav ul{list-style:none;padding-left:0}nav ul li a{text-decoration:none}nav button{position:absolute;top:0;right:0}button,.menu ul li:last-of-type a{background:transp