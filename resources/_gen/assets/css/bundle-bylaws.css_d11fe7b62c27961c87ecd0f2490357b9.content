ogle-plus svg:hover{fill:#dd4b39}main .center{grid-column-start:1;grid-column-end:-1}main .has-figure{min-height:50vh}@media screen and (min-width:640px){body>header{display:flex;justify-content:space-between}nav button{display:none;visibility:hidden}.menu{flex:1 0 auto;margin:auto}.menu ul{display:flex;flex-direction:row;visibility:visible;background-color:unset!important;position:unset!important}.menu ul li{padding:unset}.menu ul li a{font-weight:400}}@media screen and (min-width:800px){body>footer{flex-direction:row;min-height:unset}footer nav{width:33vw}}footer :not(.social) a:hover{color:#00a1de;text-decoration:underline}.give-button a{background:#00a1de;border:2px solid #00a1de;border-radius:3em;color:#fff!important;display:block;width:25vw;min-width:250px;max-width:calc(100% - 2em);margin:0 auto;text-align:center;text-decoration:none;padding:1em;box-shadow:3px 2px 1px rgba(0,0,0,.5)}.give-button a:hover{background:#c60c30;border:2px solid #c60c30;box-shadow:5px 4px 3px rgba(0,0,0,.5)}.bylaws-articles{display:block;counter-reset:article}.bylaws-articles,.bylaws-article-sections{list-style:none;padding:0}.content,.bylaws-article{counter-reset:section}.bylaws-article::before{content:"Article " counter(article,upper-roman) ". ";counter-increment:article;font-size:x-large}.bylaws-article-section::before{content:"Section " counter(section,decimal) ". ";counter-increment:section;margin-left:4ch;font-size:1.15em}.bylaws-article::before,.bylaws-article-section::before,.inline-subhead{font-weight:900}.bylaws-preamble,.bylaws-article{text-align:center}.bylaws-article ol,.bylaws-preamble p,.bylaws-article p{text-align:left}.bylaws p{text-indent:4ch}.bylaws-preamble p,.bylaws-article p,.bylaws-article-section{margin-top:1em}.bylaws h1{font-size:xx-large}.bylaws h2,.bylaws h3,.inline-subhead+p{display:inline}.bylaws-articles ol:not(.bylaws-article-sections){margin:1em auto 1em 8ch;padding:0}.inline-subhead{font-size:1.15em}